<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.c35.ptc.goout"
    android:versionCode="5"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <application
        android:name=".GoOutGlobal"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 主页-首页 -->
        <activity
            android:name=".activity.MainStartActivity"
            android:label="@string/label_activity_start"
            android:screenOrientation="portrait" />
        <!-- 项目详情 -->
        <activity
            android:name=".activity.ProjectInfoActivity"
            android:label="@string/label_activity_projectinfo"
            android:screenOrientation="portrait" />

        <!-- 顾问或中介详情 -->
        <activity
            android:name=".activity.ConsultantInfoActivity"
            android:label="@string/label_activity_consultantinfo"
            android:screenOrientation="portrait" />
        <!-- 顾问或中介发布的项目 -->
        <activity
            android:name=".activity.PublisherProjectActivity"
            android:label="@string/label_activity_publisherproject"
            android:screenOrientation="portrait" />

        <!-- 主页-院校 -->
        <activity
            android:name=".activity.MainSchoolActivity"
            android:label="@string/label_activity_school"
            android:screenOrientation="portrait" />

        <!-- 选择国家页 -->
        <activity
            android:name=".activity.SelectCountryActivity"
            android:label="@string/label_activity_select_country"
            android:screenOrientation="portrait" />
        <!-- 选择专业页 -->
        <activity
            android:name=".activity.SelectMajorActivity"
            android:label="@string/label_activity_select_major"
            android:screenOrientation="portrait" />
        <!-- 院校Tab -->
        <activity
            android:name=".activity.SchoolMainActivity"
            android:label="@string/label_activity_schoolmain"
            android:screenOrientation="portrait" />
        <!-- 院校-简介 -->
        <activity
            android:name=".activity.SchoolIntroActivity"
            android:label="@string/label_activity_schoolintro"
            android:screenOrientation="portrait" />
        <!-- 院校-专业 -->
        <activity
            android:name=".activity.SchoolMajorActivity"
            android:label="@string/label_activity_schoolmajor"
            android:screenOrientation="portrait" />
        <!-- 院校-项目 -->
        <activity
            android:name=".activity.SchoolProjectActivity"
            android:label="@string/label_activity_schoolproject"
            android:screenOrientation="portrait" />
        <!-- 院校-照片展示 -->
        <activity
            android:name=".activity.SchoolPicActivity"
            android:label="@string/label_activity_schoolpic"
            android:screenOrientation="portrait" />
        <!-- 主页-我的 -->
        <activity
            android:name=".activity.MainPersonalActivity"
            android:label="@string/label_activity_personal"
            android:screenOrientation="portrait" />
        <!-- 院校收藏 -->
        <activity
            android:name=".activity.FavouriteSchoolAcitivty"
            android:label="@string/label_activity_fav_school"
            android:screenOrientation="portrait" />
        <!-- 项目收藏 -->
        <activity
            android:name=".activity.FavouriteProjectAcitivty"
            android:label="@string/label_activity_fav_project"
            android:screenOrientation="portrait" />
        <!-- 最近联系 -->
        <activity
            android:name=".activity.PersonalRecentlyCommunicateAtivity"
            android:label="@string/label_activity_recently"
            android:screenOrientation="portrait" />

        <!-- 注册账号页 -->
        <activity
            android:name=".activity.AccountRegisterActivity"
            android:label="@string/label_activity_register"
            android:screenOrientation="portrait" />

        <!-- 账号管理页 -->
        <activity
            android:name=".activity.AccountManagerActivity"
            android:label="@string/label_activity_accountmanager"
            android:screenOrientation="portrait" />

        <!-- 账号登录页 -->
        <activity
            android:name=".activity.AccountLoginActivity"
            android:label="@string/label_activity_accountlogin"
            android:screenOrientation="portrait" />

        <!-- 分享页 -->
        <activity
            android:name=".activity.ShareActivity"
            android:label="@string/label_activity_share"
            android:screenOrientation="portrait" />

        <!-- 检查更新 -->
        <activity
            android:name=".activity.PersonalCheckUpdateActivity"
            android:label="@string/label_activity_checkupdate"
            android:screenOrientation="portrait"
            android:theme="@style/dialog" />

        <!-- 第三方认证页（含WebView） -->
        <activity
            android:name=".activity.ThirdAuthActivity"
            android:label="@string/label_activity_thirdoath"
            android:screenOrientation="portrait" />

        <uses-library android:name="android.test.runner" />
    </application>

    <instrumentation
        android:name="android.test.InstrumentationTestRunner"
        android:targetPackage="com.c35.ptc.goout" />

</manifest>